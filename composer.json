{
    "name": "macramejs/admin-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Carstens-Behrens",
            "email": "lennart.carbe@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Macrame\\Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "publishes/app/",
            "Admin\\": "publishes/admin/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "php": "^8.1",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0",
        "laravel/sanctum": "^2.15"
    },
    "require": {
        "illuminate/support": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/filesystem": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Macrame\\Admin\\AdminServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
