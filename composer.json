{
    "name": "macramejs/cms-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Carstens-Behrens",
            "email": "lennart.carbe@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Macrame\\CMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "php": "^8.1",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0"
    },
    "require": {
        "illuminate/support": "^9.0",
        "illuminate/contracts": "^9.0",
        "inertiajs/inertia-laravel": "^0.5",
        "laravel/breeze": "^1.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Macrame\\CMS\\CmsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
